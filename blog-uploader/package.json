{
  "name": "internal-publisher",
  "version": "0.1.0",
  "private": true,
  "description": "Internal desktop publisher for SalonBox blog sources",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev:build": "tsc -w -p tsconfig.json",
    "dev:electron": "electronmon .",
    "dev": "concurrently -k \"npm:dev:build\" \"npm:dev:electron\"",
    "start": "npm run build && electron ."
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.978.0",
    "@aws-sdk/client-codebuild": "^3.662.0",
    "@aws-sdk/client-s3": "^3.662.0",
    "@aws-sdk/client-sts": "^3.662.0",
    "dotenv": "^17.2.3",
    "electron": "^32.2.6",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "mdast-util-to-string": "^4.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.16.11",
    "concurrently": "^9.0.1",
    "electronmon": "^2.0.2",
    "typescript": "^5.6.3"
  }
}
